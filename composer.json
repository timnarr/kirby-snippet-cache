{
	"name": "timnarr/kirby-snippet-cache",
	"description": "This plugin provides caching functionality for Kirby CMS snippets.",
	"type": "kirby-plugin",
	"version": "0.1.0",
	"license": "MIT",
	"homepage": "https://github.com/timnarr/kirby-snippet-cache",
	"authors": [
		{
			"name": "Tim Narr",
			"email": "dev@tim-narr.com"
		}
	],
	"require": {
		"php": ">=8.0.0",
		"getkirby/composer-installer": "^1.2"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3"
	},
	"scripts": {
		"lint": ["php-cs-fixer fix --dry-run"],
		"lint:fix": ["php-cs-fixer fix"]
	},
	"config": {
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	}
}
